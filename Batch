:EODSRVGRP
@echo ***** CREATING END OF DAY-MONTH SERVICE *****
ping 127.0.0.1 -n 3 > nul
xcopy /h "\\%SERVER IP%\%NETSHARE%\GENESIS\genEOD%GENGRP%.exe" "C:\GENESIS\GENSERVICES" /D /y
(
	ECHO echo off
	ECHO SETLOCAL EnableExtensions
	ECHO cls
	ECHO echo ***** KILL EOD-EOM TASK *****
	ECHO set EOD1=genEOD%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EOD1%%"'^) DO IF %%%%x == %%EOD1%% goto EODop1
	ECHO echo %%EOD1%% SERVICE IS NOT RUNNING
	ECHO goto STEOD
	ECHO :EODop1
	ECHO taskkill /IM %%EOD1%% /f
	ECHO :STEOD
	ECHO echo off
	ECHO cls
	ECHO If "%%1"=="" goto gdate
	ECHO set date="%%1"
	ECHO goto start
	ECHO :gdate
	ECHO set /p date=Give Date:
	ECHO :start
	ECHO ======================
	ECHO set GENGRPNAME=%GENGRPNAME%
	ECHO set GENGRP=%GENGRP%
	ECHO set RUNDATE=%%date%%
	ECHO ======================
	ECHO SETLOCAL EnableExtensions
	ECHO set EXE=genEOD%%GENGRP%%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXE%%"'^) DO IF %%%%x == %%EXE%% goto FOUND
	ECHO echo SERVICE IS NOT RUNNING
	ECHO echo ***** STARTING EOD-EOM SERVICE *****
	ECHO echo %%GENGRP%%
	ECHO copy "\\%SERVER IP%\%NETSHARE%\GENESIS\GENSERVICES\genEOD.exe" "C:\GENESIS\GENSERVICES\genEOD%%GENGRP%%.exe" /y
	ECHO start "" "C:\GENESIS\GENSERVICES\%%EXE%%" CAT %%GENGRP%% %%RUNDATE%%
	ECHO goto EXIT
	ECHO :FOUND
	ECHO echo SERVICE IS RUNNING
	ECHO ping 127.0.0.1 -n 3 ^> nul
	ECHO exit
)>"C:\GENESIS\GENSERVICES\START-EOD-EOM-SERVICE-%GENGRP%.bat"
schtasks /create /sc DAILY /st 21:00:00 /tn "genesisEOD-EOM-%GENGRP%" /RU "%SVRNAME%" /RP "%SVRPASS%" /tr "C:\GENESIS\GENSERVICES\START-EOD-EOM-SERVICE-%GENGRP%.bat %%DATE%%"
cls
@echo ***** DONE *****
@echo ***** CREATING CHECK SERVICES STATUS *****
(
	ECHO @echo off
	ECHO SETLOCAL EnableExtensions
	ECHO set EXE1=gen*
	ECHO :top
	ECHO cls
	ECHO TASKLIST /FI "IMAGENAME eq %%exe1%%"
	ECHO @ECHO .
	ECHO @ECHO .
	ECHO @ECHO .
	ECHO timeout 60
	ECHO goto top
)>"C:\GENESIS\GENSERVICES\CHECK-SERVICES-STATUS.bat"

echo Set oWS = WScript.CreateObject("WScript.Shell") > CreateShortcut.vbs
