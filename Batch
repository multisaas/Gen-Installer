	ECHO echo sLinkFile = "%%allusersprofile%%\Desktop\GENESIS OFFLINE TILL.lnk" ^>^> CreateShortcut.vbs
	ECHO echo Set oLink = oWS.CreateShortcut^(sLinkFile^) ^>^> CreateShortcut.vbs
	ECHO echo oLink.TargetPath = "%%userprofile%%\AppData\Local\GENESIS\GenOTL.exe" ^>^> CreateShortcut.vbs
	ECHO echo olink.workingdirectory = "%%userprofile%%\AppData\Local\GENESIS" ^>^> CreateShortcut.vbs
	ECHO echo olink.IconLocation ="%%userprofile%%\AppData\Local\GENESIS\GenOTL.exe" ^>^> CreateShortcut.vbs
	ECHO echo oLink.Save ^>^> CreateShortcut.vbs
	ECHO cscript CreateShortcut.vbs
	ECHO del CreateShortcut.vbs
	ECHO :fnf3
	ECHO @echo ***** SETTING THE TIME *****
	ECHO @echo ***** SETTING THE TIME ***** ^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	ECHO net time \\%%SERVER IP%% /set /yes ^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	ECHO @echo ***** IMPORT TIME SETTINGS*****
	ECHO @echo ***** IMPORT TIME SETTINGS***** ^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	ECHO Reg import "C:\GENESIS\INSTALL\REGION.REG" ^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	
	ECHO :-----------------------------------------------------------------------
	ECHO @echo ***** CHECKING SQL SERVICE *****
	ECHO @echo ***** CHECKING SQL SERVICE ***** %%date%% %%time%% ^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	ECHO ping 127.0.0.1 -n 2 ^> nul
	ECHO for /F "tokens=3 delims=: " %%^%%H in ^('sc query "MSSQLSERVER" ^^^| findstr "STATE"'^) do (if /I "%%%%H" NEQ "RUNNING" (net start "MSSQLSERVER"^)^)^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	ECHO sc query "MSSQLSERVER"^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
	ECHO sc query "MSSQLSERVER" ^| findstr RUNNING
	ECHO ping 127.0.0.1 -n 3 ^> nul
	ECHO exit
) >"%NETDRIVE%:\GENESIS\GENESIS INSTALL FILES\GenesisLogonUser.bat"
ping 127.0.0.1 -n 3 > nul

