:EODSERVICE
@echo ***** CREATING END OF DAY-MONTH SERVICE *****
ping 127.0.0.1 -n 3 > nul
xcopy /h "\\%SERVER IP%\%NETSHARE%\GENESIS\genEOD%GENBR%.exe" "C:\GENESIS\GENSERVICES" /D /y
(
	ECHO echo off
	ECHO SETLOCAL EnableExtensions
	ECHO cls
	ECHO echo ***** KILL EOD-EOM TASK *****
	ECHO set EOD1=genEOD%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EOD1%%"'^) DO IF %%%%x == %%EOD1%% goto EODop1
	ECHO echo %%EOD1%% SERVICE IS NOT RUNNING
	ECHO goto STEOD
	ECHO :EODop1
	ECHO taskkill /IM %%EOD1%% /f
	ECHO :STEOD
	ECHO echo off
	ECHO cls
	ECHO If "%%1"=="" goto gdate
	ECHO set date="%%1"
	ECHO goto start
	ECHO :gdate
	ECHO set /p date=Give Date:
	ECHO :start
	ECHO ======================
	ECHO set GENCOMP=%GENCOMP%
	ECHO set GENBR=%GENBR%
	ECHO set RUNDATE=%%date%%
	ECHO ======================
	ECHO SETLOCAL EnableExtensions
	ECHO set EXE=genEOD%%GENBR%%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXE%%"'^) DO IF %%%%x == %%EXE%% goto FOUND
	ECHO echo SERVICE IS NOT RUNNING
	ECHO echo ***** STARTING EOD-EOM SERVICE *****
	ECHO echo %%GENBR%%
	ECHO copy "\\%SERVER IP%\%NETSHARE%\GENESIS\GENSERVICES\genEOD.exe" "C:\GENESIS\GENSERVICES\genEOD%%GENBR%%.exe" /y
	ECHO start "" "C:\GENESIS\GENSERVICES\%%EXE%%" %%GENCOMP%% %%GENBR%% %%RUNDATE%%
	ECHO goto EXIT
	ECHO :FOUND
	ECHO echo SERVICE IS RUNNING
	ECHO ping 127.0.0.1 -n 3 ^> nul
	ECHO exit
)>"C:\GENESIS\GENSERVICES\START-EOD-EOM-SERVICE-%GENBR%.bat"
schtasks /create /sc DAILY /st 21:00:00 /tn "genesisEOD-EOM-%GENBR%" /RU "%SVRNAME%" /RP "%SVRPASS%" /tr "C:\GENESIS\GENSERVICES\START-EOD-EOM-SERVICE-%GENBR%.bat %%DATE%%"
cls
@echo ***** DONE *****
@echo ***** CREATING CHECK SERVICES STATUS *****
(
	ECHO @echo off
	ECHO SETLOCAL EnableExtensions
	ECHO set EXE1=gen*
	ECHO :top
	ECHO cls
	ECHO TASKLIST /FI "IMAGENAME eq %%exe1%%"
	ECHO @ECHO .
	ECHO @ECHO .
	ECHO @ECHO .
	ECHO timeout 60
	ECHO goto top
)>"C:\GENESIS\GENSERVICES\CHECK-SERVICES-STATUS.bat"

echo Set oWS = WScript.CreateObject("WScript.Shell") > CreateShortcut.vbs
echo sLinkFile = "%HOMEDRIVE%%HOMEPATH%\Desktop\CHECK SERVICES STATUS.lnk" >> CreateShortcut.vbs
echo Set oLink = oWS.CreateShortcut(sLinkFile) >> CreateShortcut.vbs
echo oLink.TargetPath = "C:\GENESIS\GENSERVICES\CHECK-SERVICES-STATUS.bat" >> CreateShortcut.vbs
echo olink.workingdirectory = "C:\GENESIS\GENSERVICES\" >> CreateShortcut.vbs
echo olink.IconLocation ="C:\GENESIS\INSTALL\LOGO.ico" >> CreateShortcut.vbs
echo oLink.Save >> CreateShortcut.vbs
cscript CreateShortcut.vbs
del CreateShortcut.vbs

ping 127.0.0.1 -n 3 > nul
goto TOPGENSVR

:GRPSERVICE
cls
ECHO =========================================
ECHO PLEASE PROVIDE SERVER DETAIL THATS NEEDED
ECHO =========================================
set /p SVRNAME=SERVER USERNAME:
set /p SVRPASS=SERVER PASSWORD:
set /p GENGRPNAME=GENESIS GROUP NAME:
set /p GENGRP=STORE GROUP CODE:
(
	ECHO @echo off
	ECHO SETLOCAL EnableExtensions
	ECHO cls
	ECHO echo ***** CLOSING ALL OPEN SERVICES *****
	ECHO set EXEEOD1=genEOD%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEEOD1%%"'^) DO IF %%%%x == %%EXEEOD1%% goto EODop1
	ECHO echo %%EXEEOD1%% SERVICE IS NOT RUNNING
	ECHO goto EXEBRS1
	ECHO :EODop1
	ECHO taskkill /IM %%EXEEOD1%%
	
	ECHO :EXEBRS1
	ECHO set EXEBRS1=GENGRPServer%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEBRS1%%"'^) DO IF %%%%x == %%EXEBRS1%% goto BRSop1
	ECHO echo %%EXEBRS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEHQS1
	ECHO :BRSop1
	ECHO taskkill /IM %%EXEBRS1%% /F
	
	ECHO :EXEHQS1
	ECHO set EXEHQS1=genHQServer%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEHQS1%%"'^) DO IF %%%%x == %%EXEHQS1%% goto HQSop1
	ECHO echo %%EXEHQS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEBRA1
	ECHO :HQSop1
	ECHO taskkill /IM %%EXEHQS1%% /F
	
	ECHO :EXEBRA1
	ECHO set EXEBRA1=GENGRPanchSvr%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEBRA1%%"'^) DO IF %%%%x == %%EXEBRA1%% goto BRAop1
	ECHO echo %%EXEBRA1%% SERVICE IS NOT RUNNING
	ECHO goto EXEHOS1
	ECHO :BRAop1
	ECHO taskkill /IM %%EXEBRA1%% /F
	
	ECHO :EXEHOS1
	ECHO set EXEHOS1=genHOSvr%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEHOS1%%"'^) DO IF %%%%x == %%EXEHOS1%% goto HOSop1
	ECHO echo %%EXEHOS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEWDS1
	ECHO :HOSop1
	ECHO taskkill /IM %%EXEHOS1%% /F
	
	ECHO :EXEWDS1
	ECHO set EXEWDS1=genDownloadSvr%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEWDS1%%"'^) DO IF %%%%x == %%EXEWDS1%% goto WDSop1
	ECHO echo %%EXEWDS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEWUS1
	ECHO :WDSop1
	ECHO taskkill /IM %%EXEWDS1%% /F
	
	ECHO :EXEWUS1
	ECHO set EXEWUS1=genUploadSvr%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEWUS1%%"'^) DO IF %%%%x == %%EXEWUS1%% goto WUSop1
	ECHO echo %%EXEWUS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEHQSV1
	ECHO :WUSop1
	ECHO taskkill /IM %%EXEWUS1%% /F

	ECHO :EXEHQSV1
	ECHO set EXEHQSV1=genHQServer%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEHQSV1%%"'^) DO IF %%%%x == %%EXEHQSV1%% goto ICSSop1
	ECHO echo %%EXEHQSV1%% SERVICE IS NOT RUNNING
	ECHO goto EXEICSS1
	ECHO :ICSSop1
	ECHO taskkill /IM %%EXEHQSV1%% /F
	
	ECHO :EXEICSS1
	ECHO set EXEICSS1=genICStoreSvr%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEICSS1%%"'^) DO IF %%%%x == %%EXEICSS1%% goto ICSSop1
	ECHO echo %%EXEICSS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEICHS1
	ECHO :ICSSop1
	ECHO taskkill /IM %%EXEICSS1%% /F
	
	ECHO :EXEICHS1
	ECHO set EXEICHS1=genICHQSvr%GENGRP%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEICHS1%%"'^) DO IF %%%%x == %%EXEICHS1%% goto ICHSop1
	ECHO echo %%EXEICHS1%% SERVICE IS NOT RUNNING
	ECHO goto ICHsnf1
	ECHO :ICHSop1
	ECHO taskkill /IM %%EXEICHS1%% /F
	ECHO :ICHsnf1
	ECHO timeout 60
	ECHO EXIT
)>"C:\GENESIS\GENSERVICES\STOP-ALL-SERVICES-%GENGRP%.bat"
schtasks /create /sc DAILY /st 06:00:00 /RI 240 /DU 24:00 /tn "GENESIS STOP ALL SERVICES %GENGRP%" /RU "%SVRNAME%" /RP "%SVRPASS%" /tr "C:\GENESIS\GENSERVICES\STOP-ALL-SERVICES-%GENGRP%.bat"
sqlcmd -U sa -P Pa$$w0rD -S (local) -Q "CREATE DATABASE [GENESIS-%GENGRP%-VPN]"


:TOPGENGRPSVR
cls
ECHO USERNAME:%SVRNAME% USERPASSWORD:%SVRPASS%
cls
ECHO USERNAME:%SVRNAME% USERPASSWORD:%SVRPASS%
ECHO GENGRPNAME: %GENGRPNAME%
ECHO GENESIS GROUP CODE: %GENGRP%
ECHO.
ECHO ........................................................................
ECHO SELECT YOUR OPTION BY TYPING THE CORESPONDING NUMBER AND THEN ENTER KEY.
ECHO ........................................................................
ECHO.
ECHO  1 - CREATE IC STORE SERVICE
ECHO  2 - CREATE IC HEAD OFFICE SERVICE
::ECHO  3 - CREATE HEAD OFFICE SERVICE
::ECHO  4 - CREATE BRANCH SERVICES
::ECHO  5 - CREATE WEB SERVICE
::ECHO  6 - CREATE HO SERVICE (KITKAT)
::ECHO  7 - CREATE BRANCH SERVICE (KITKAT)
ECHO  3 - CREATE HEAD OFFICE SERVER SERVICE (OFFLINE TIIL)
::ECHO  9 - CREATE END OF DAY SERVICE
ECHO 10 - UPDATE DETAILS
ECHO 99 - MAIN MENU

ECHO.
SET /P option=Select a task then press ENTER:
IF %option%==1 GOTO ICSTSRVGRP
IF %option%==2 GOTO ICHQSRVGRP
::IF %option%==3 GOTO HQSRVGRP
::IF %option%==4 GOTO BRSRVGRP
::IF %option%==5 GOTO WEBSRVGRP
::IF %option%==6 GOTO HOSRVGRP
::IF %option%==7 GOTO BRASRVGRP
IF %option%==3 GOTO HOOSRVGRP
::IF %option%==9 GOTO EODSRVGRP
IF %option%==10 GOTO GRPSERVICE
IF %option%==99 GOTO MENU
IF %option%=="%option%" GOTO TOPGENGRPSVR
goto TOPGENGRPSVR

:ICSTSRVGRP
@echo ***** CREATING IC STORE SYNC SERVICE*****
copy "\\%SERVER IP%\%NETSHARE%\GENESIS\GENSERVICES\genICStoreSvr.exe" "C:\GENESIS\GENSERVICES\genICStoreSvr%GENGRP%.exe" /y
(
	ECHO echo off
	ECHO cls
	ECHO ======================
	ECHO set GENGRPNAME=%GENGRPNAME%
	ECHO set GENGRP=%GENGRP%
	ECHO ======================
