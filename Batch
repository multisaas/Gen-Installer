xcopy /h "\\%SERVER IP%\%NETSHARE%\Genesis\GENESIS INSTALL FILES\REGION.reg" "C:\Genesis\INSTALL" /D /y
::xcopy /h "\\%SERVER IP%\%NETSHARE%\GENESIS\GENESIS INSTALL FILES\GENESISLogon.bat" "%allusersprofile%\Start Menu\Programs\StartUp\GENESISLogon.bat" /D /y )
