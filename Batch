:CREATEBACKUP
cls
@ECHO *****DO YOU WHANT TO CREATE A BACKUP TASK*****
set /P INPUT=TYPE INPUT: %=%
If %INPUT%==y goto yes9
If %INPUT%==Y goto yes9
If %INPUT%== %INPUT% goto MENU
:yes9
cls
@ECHO *****SETUP BACKUP DETAILS*****
cls
@echo (Please keep in mind Common database will backup automatically)
set /P NODBs=HOW MANY DATABASES WILL YOU BACKUP(MAX NUMBER 4)?:
if %NODBs%==1 goto 1DB
if %NODBs%==2 goto 2DB
if %NODBs%==3 goto 3DB
if %NODBs%==4 goto 4DB
if %NODBs%==%NODBs% goto CREATEBACKUP
:1DB
set /p SERVERDB1=TYPE DataBase NAME:
goto next1
:2DB
set /p SERVERDB1=TYPE 1st DataBase NAME:
set /p SERVERDB2=TYPE 2nd DataBase NAME:
goto next1
:3DB
set /p SERVERDB1=TYPE 1st DataBase NAME:
set /p SERVERDB2=TYPE 2nd DataBase NAME:
set /p SERVERDB3=TYPE 3rd DataBase NAME:
goto next1
:4DB
set /p SERVERDB1=TYPE 1st DataBase NAME:
set /p SERVERDB2=TYPE 2nd DataBase NAME:
set /p SERVERDB3=TYPE 3rd DataBase NAME:
set /p SERVERDB4=TYPE 4th DataBase NAME:
:next1
set /p NOD=FOR HOW MANY DAYS DO YOU NEED BACKUPS? SELECT FROM 1 TO 6:
set /p username=TYPE SERVER USERNAME:
set /p password=TYPE SERVER PASSWORD:
set /p BACKUPDIR1=TYPE BACKUP LOCATION eg.(c:\backup):
@ECHO =================================================
@ECHO *****SETUP EMAIL DETAILS*****
set /p smtpsvr=OUTGONING SMTP SERVER: 
set /p smtpp=OUTGONING SMTP PORT: 
set /p fromemail=EMAIL ADDRESS SENDING FROM: 
set /p mailuser=EMAIL USERNAME: 
set /p mailpass=EMAIL USER PASSWORD: 
set /p sendto=SEND TO EMAIL ADDRESSES(name@domain.com;name2@domain2.com): 
set /p subject=PLEASE PROVIDE COMPANY NAME: 

cls
Echo ==================================================
ECHO BACKUP WILL BE CREATED WITH THE FOLLOWING DETAILS.
Echo ==================================================
ECHO DATABASE THAT WILL BACKUP
ECHO -------------
ECHO %serverdb1%
ECHO %serverdb2%
ECHO %serverdb3%
ECHO %serverdb4%
ECHO -------------
ECHO NUMBER OF DAYS TO KEEP BACKUP
ECHO (%NOD%)
ECHO -------------
ECHO SERVER USERNAME
ECHO (%username%)
ECHO AND THE PASSWORD
ECHO (%password%)
ECHO -------------
ECHO EMAIL NOTIFICATIONS SETTINS
ECHO SMTP SERVER (%smtpsvr%) SMTP PORT (%smtpp%) SMTP MAIL ADDRESS (%fromemail%)
ECHO EMAIL ACCOUNT USERNAME (%mailuser%) AND PASSWORD (%mailpass%)
ECHO EMAIL ADDRESS TO SEND NOTIFICATIONS TO (%sendto%)
ECHO -------------
ECHO AND THE BACKUP WILL BE SAVED AT THIS LOCATION (%BACKUPDIR1%\RUNGENBACKUP)
set /P INPUT=IS THIS DETAILS CORRECT (Y/N): %=%
If %INPUT%==y goto yes8
If %INPUT%==Y goto yes8
If %INPUT% == %INPUT% goto CREATEBACKUP
:yes8
md "C:\SQLBACKUPS\INSTALL"
xcopy /h "\\%SERVER IP%\%NETSHARE%\GENESIS\GENESIS INSTALL FILES\email64bit.exe" "C:\SQLBACKUPS\INSTALL" /D /Y
xcopy /h "\\%SERVER IP%\%NETSHARE%\GENESIS\GENESIS INSTALL FILES\WINRAR.exe" "C:\GENESIS\INSTALL" /D /Y
if exist "C:\Program Files (x86)\WinRAR\rar.exe" goto rarreddy
CLS
ECHO ********************************
ECHO * WINRAR 32bit IS NOT INSTALLED*
ECHO ********************************
ECHO *****Installing WINRAR 32bit*****
"C:\GENESIS\INSTALL\WINRAR.exe" /passive
:rarreddy
ECHO *****CREATING BACKUP FILES*****
if exist "%BACKUPDIR1%\GEN BACKUP FILES" goto nexthop1
md "%BACKUPDIR1%\GEN BACKUP FILES"
:nexthop1
if exist "C:\SQLBACKUPS\INSTALL" goto nexthop2
md "C:\SQLBACKUPS\INSTALL"
:nexthop2
CLS
@echo CREATING BACKUP FILE
ECHO setlocal enabledelayedexpansion>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO @echo off>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO CLS>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo !date! !time!>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo _________________________>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo   HOURLY BACKUP RUNNING>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo _________________________>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
if %NODBs%==1 goto 1DB1
if %NODBs% GEQ 2 goto 2DB1
:1DB1
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB1%.bak" goto COM1>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
goto done
:2DB1
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB1%.bak" goto DB2>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
:done
ECHO echo FULL>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO ^>%BACKUPDIR1%\BACKUPlog.txt (>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo _________________________>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo  FULL BACKUP STARTED AT>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo  !date!>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo  !time!>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo _________________________>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
if %NODBs%==1 goto 1DB2
if %NODBs%==2 goto 2DB2
if %NODBs%==3 goto 3DB2
if %NODBs%==4 goto 4DB2
:1DB2
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB1%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB1%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
goto next2
:2DB2
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB1%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB1%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO :DB2>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB2%.bak" goto COM1>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB2%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB2%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
goto next2
:3DB2
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB1%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB1%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO :DB2>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB2%.bak" goto DB3>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB2%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB2%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO :DB3>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB3%.bak" goto COM1>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB3%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB3%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
goto next2
:4DB2
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB1%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB1%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO :DB2>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB2%.bak" goto DB3>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB2%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB2%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO :DB3>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB3%.bak" goto DB4>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB3%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB3%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO :DB4>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB4%.bak" goto COM1>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [%SERVERDB4%] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-%SERVERDB4%.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
:next2
ECHO :COM1>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO if exist "%BACKUPDIR1%\GEN BACKUP FILES\TODAY-GENESISCOMMON.bak" goto diff>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO SqlCmd -E -S %server ip% -Q "BACKUP DATABASE [GENESISCOMMON] TO DISK= '%BACKUPDIR1%\GEN BACKUP FILES\TODAY-GENESISCOMMON.bak'">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO forfiles -p "%BACKUPDIR1%\GEN BACKUP FILES" -s -m * -d -%NOD% -c "cmd /c del @FILE">>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
ECHO echo ________________________>>"%BACKUPDIR1%\RUNGENBACKUP.BAT"
