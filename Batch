=============================================================================================================
:PC NAME CHANGE
cls
@echo ***** DO YOU WHANT TO CHANGE PC NAME? *****(Y/N)
set INPUT=
set /P INPUT=TYPE INPUT: %=%
If %INPUT% == y goto yes2
If %INPUT% == Y goto yes2
If %INPUT% == %INPUT% goto MENU
:yes2
set /p id=NEW PC NAME:
@echo %id%
wmic computersystem  where caption="%computername%" rename "%id%" >>"c:\scripts\installlog.txt"
shutdown -r -t 3
exit
=============================================================================================================
:PREREQUISITES
cls
@echo ***** INSTALL SQL PREREQUISITES? *****(Y/N)
set INPUT=
set /P INPUT=TYPE INPUT: %=%
If %INPUT% == y goto yes5
If %INPUT% == Y goto yes5
If %INPUT% == %INPUT% goto MENU
:yes5
echo. 
@echo ***** INSTALLING WINDOWS INSTALLER 4.5 *****
@echo ***** INSTALLING WINDOWS INSTALLER 4.5 ***** >>"c:\scripts\installlog.txt"
"C:\GENESIS\INSTALL\Windows Installer v4.5.exe" /passive /log:"c:\scripts\installlog.txt"
echo.
echo.
@echo ***** INSTALLING POWERSHELL *****
@echo ***** INSTALLING POWERSHELL ***** >>"c:\scripts\installlog.txt"
"C:\GENESIS\INSTALL\WindowsXP-KB968930-x86-ENG.exe" /passive /log:"c:\scripts\installlog.txt"
echo.
echo.
@echo ***** INSTALLING .NetFrameworks 3.5 *****
@echo ***** INSTALLING .NetFrameworks 3.5 ***** >>"c:\scripts\installlog.txt"
"C:\GENESIS\INSTALL\dotnetfx35.exe" /passive /log:"c:\scripts\installlog.txt"
goto MENU
=============================================================================================================
:Express08
cls
@echo ***** DO YOU WANT TO INSTALL SQL 2008? *****(Y/N)
set INPUT=
set /P INPUT=TYPE INPUT: %=%
If %INPUT% == y goto yes1
If %INPUT% == Y goto yes1
If %INPUT% == %INPUT% goto MENU
:yes1
cls
@echo ***** NOW INSTALING SQL EXPRESS 2008 *****
@echo ***** NOW INSTALING SQL EXPRESS 2008 ***** >>"c:\scripts\installlog.txt"
"C:\GENESIS\INSTALL\SQL EXPRESS 2008\SQLEXPRWT_x86_ENU.exe" /Q /ACTION=Install /FEATURES=SQLENGINE,SSMS /INSTANCENAME=MSSQLServer /SECURITYMODE=SQL /SAPWD="Pa$$w0rD" /SQLSYSADMINACCOUNTS="%COMPUTERNAME%\%USERNAME%" /SQLSVCACCOUNT="NT AUTHORITY\SYSTEM" /TCPENABLED=1 /NPENABLED=1 /ERRORREPORTING=0 /IACCEPTSQLSERVERLICENSETERMS
ping 127.0.0.1 -n 3 > nul
@echo *****STARTING SQL SERVICE***** >>"c:\scripts\installlog.txt"
for /F "tokens=3 delims=: " %%H in ('sc query "MSSQLSERVER" ^| findstr "STATE"') do (if /I "%%H" NEQ "RUNNING" (net start "MSSQLSERVER")) >>"c:\scripts\installlog.txt"
ping 127.0.0.1 -n 3 > nul
"\\%SERVER IP%\%NETSHARE%\GENESIS\GENESIS INSTALL FILES\GENESIS-INSTALL.BAT"
exit
