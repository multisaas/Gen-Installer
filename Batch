	ECHO ping 127.0.0.1 -n 2 ^> nul
	ECHO for /F "tokens=3 delims=: " %%^%%H in ^('sc query "MSSQLSERVER" ^^^| findstr "STATE"'^) do (if /I "%%%%H" NEQ "RUNNING" (net start "MSSQLSERVER"^)^)^>^>"%%userprofile%%\Documents\scripts\logonlog.txt"
