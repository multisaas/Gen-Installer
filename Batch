:GENSERVICES
md c:\GENESIS\GENSERVICES
md c:\GENESIS\INSTALL
md %NETDRIVE%:\GENESIS\GENSERVICES
COLOR 07
cls

:UPDSERVICE
ECHO ........................................................................
ECHO SELECT YOUR OPTION BY TYPING THE CORESPONDING NUMBER AND THEN ENTER KEY.
ECHO ........................................................................
ECHO.
ECHO  1 - CREATE SERVICE SINGLE 
ECHO  2 - CREATE SERVICE GROUP
ECHO 10 - UPDATE DETAILS
ECHO 99 - MAIN MENU

ECHO.
SET /P option=Select a task then press ENTER:
IF %option%==1 GOTO ONESERVICE
IF %option%==2 GOTO GRPSERVICE
IF %option%==10 GOTO UPDSERVICE
IF %option%==99 GOTO MENU
IF %option%=="%option%" GOTO UPDSERVICE
goto UPDSERVICE


:ONESERVICE
cls
ECHO =========================================
ECHO PLEASE PROVIDE SERVER DETAIL THATS NEEDED
ECHO =========================================
set /p SVRNAME=SERVER USERNAME:
set /p SVRPASS=SERVER PASSWORD:
set /p GENCOMP=GENESIS COMPANY CODE:
set /p GENBR=GENESIS BRANCH CODE:
::set /p GENGRP=Store GROUP CODE:
(
	ECHO @echo off
	ECHO SETLOCAL EnableExtensions
	ECHO cls
	ECHO echo ***** CLOSING ALL OPEN SERVICES *****
	ECHO set EXEEOD1=genEOD%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEEOD1%%"'^) DO IF %%%%x == %%EXEEOD1%% goto EODop1
	ECHO echo %%EXEEOD1%% SERVICE IS NOT RUNNING
	ECHO goto EXEBRS1
	ECHO :EODop1
	ECHO taskkill /IM %%EXEEOD1%%
	
	ECHO :EXEBRS1
	ECHO set EXEBRS1=genBRServer%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEBRS1%%"'^) DO IF %%%%x == %%EXEBRS1%% goto BRSop1
	ECHO echo %%EXEBRS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEHQS1
	ECHO :BRSop1
	ECHO taskkill /IM %%EXEBRS1%% /F
	
	ECHO :EXEHQS1
	ECHO set EXEHQS1=genHQServer%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEHQS1%%"'^) DO IF %%%%x == %%EXEHQS1%% goto HQSop1
	ECHO echo %%EXEHQS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEBRA1
	ECHO :HQSop1
	ECHO taskkill /IM %%EXEHQS1%% /F
	
	ECHO :EXEBRA1
	ECHO set EXEBRA1=genBranchSvr%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEBRA1%%"'^) DO IF %%%%x == %%EXEBRA1%% goto BRAop1
	ECHO echo %%EXEBRA1%% SERVICE IS NOT RUNNING
	ECHO goto EXEHOS1
	ECHO :BRAop1
	ECHO taskkill /IM %%EXEBRA1%% /F
	
	ECHO :EXEHOS1
	ECHO set EXEHOS1=genHOSvr%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEHOS1%%"'^) DO IF %%%%x == %%EXEHOS1%% goto HOSop1
	ECHO echo %%EXEHOS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEWDS1
	ECHO :HOSop1
	ECHO taskkill /IM %%EXEHOS1%% /F
	
	ECHO :EXEWDS1
	ECHO set EXEWDS1=genDownloadSvr%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEWDS1%%"'^) DO IF %%%%x == %%EXEWDS1%% goto WDSop1
	ECHO echo %%EXEWDS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEWUS1
	ECHO :WDSop1
	ECHO taskkill /IM %%EXEWDS1%% /F
	
	ECHO :EXEWUS1
	ECHO set EXEWUS1=genUploadSvr%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEWUS1%%"'^) DO IF %%%%x == %%EXEWUS1%% goto WUSop1
	ECHO echo %%EXEWUS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEHQSV1
	ECHO :WUSop1
	ECHO taskkill /IM %%EXEWUS1%% /F

	ECHO :EXEHQSV1
	ECHO set EXEHQSV1=genHQServer%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEHQSV1%%"'^) DO IF %%%%x == %%EXEHQSV1%% goto ICSSop1
	ECHO echo %%EXEHQSV1%% SERVICE IS NOT RUNNING
	ECHO goto EXEICSS1
	ECHO :ICSSop1
	ECHO taskkill /IM %%EXEHQSV1%% /F
	
	ECHO :EXEICSS1
	ECHO set EXEICSS1=genICStoreSvr%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEICSS1%%"'^) DO IF %%%%x == %%EXEICSS1%% goto ICSSop1
	ECHO echo %%EXEICSS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEWCS1
	ECHO :ICSSop1
	ECHO taskkill /IM %%EXEICSS1%% /F
	
	ECHO :EXEWCS1
	ECHO set EXEWCS1=genWCS%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEWCS1%%"'^) DO IF %%%%x == %%EXEWCS1%% goto ICSSop1
	ECHO echo %%EXEWCS1%% SERVICE IS NOT RUNNING
	ECHO goto EXEICHS1
	ECHO :ICSSop1
	ECHO taskkill /IM %%EXEWCS1%% /F
	
	ECHO :EXEICHS1
	ECHO set EXEICHS1=genICHQSvr%GENBR%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXEICHS1%%"'^) DO IF %%%%x == %%EXEICHS1%% goto ICHSop1
	ECHO echo %%EXEICHS1%% SERVICE IS NOT RUNNING
	ECHO goto ICHsnf1
	ECHO :ICHSop1
	ECHO taskkill /IM %%EXEICHS1%% /F
	ECHO :ICHsnf1
	ECHO timeout 60
	ECHO EXIT
)>"C:\GENESIS\GENSERVICES\STOP-ALL-SERVICES-%GENBR%.bat"
schtasks /create /sc DAILY /st 06:00:00 /RI 240 /DU 24:00 /tn "GENESIS STOP ALL SERVICES %GENBR%" /RU "%SVRNAME%" /RP "%SVRPASS%" /tr "C:\GENESIS\GENSERVICES\STOP-ALL-SERVICES-%GENBR%.bat"
sqlcmd -U sa -P Pa$$w0rD -S (local) -Q "CREATE DATABASE [GENESIS-%GENBR%-VPN]"

:TOPGENSVR
cls
ECHO USERNAME:%SVRNAME% USERPASSWORD:%SVRPASS%
:UPDCOBRDET
ECHO GENESIS COMPANY CODE:%GENCOMP%
ECHO GENESIS BRANCH CODE:%GENBR%
ECHO.
ECHO ........................................................................
ECHO SELECT YOUR OPTION BY TYPING THE CORESPONDING NUMBER AND THEN ENTER KEY.
ECHO ........................................................................
ECHO.
ECHO  1 - CREATE IC STORE SERVICE
ECHO  2 - CREATE IC HEAD OFFICE SERVICE
ECHO  3 - CREATE HEAD OFFICE SERVICE
ECHO  4 - CREATE BRANCH SERVICES
ECHO  5 - CREATE WEB SERVICE
ECHO  6 - CREATE HO SERVICE (KITKAT)
ECHO  7 - CREATE BRANCH SERVICE (KITKAT)
ECHO  8 - CREATE HEAD OFFICE SERVER SERVICE (OFFLINE TIIL)
ECHO  9 - CREATE END OF DAY SERVICE
ECHO 10 - UPDATE DATAILS
ECHO 99 - MAIN MENU

ECHO.
SET /P option=Select a task then press ENTER:
IF %option%==1 GOTO ICSTSERVICE
IF %option%==2 GOTO ICHQSERVICE
IF %option%==3 GOTO HQSERVICE
IF %option%==4 GOTO BRSERVICE
IF %option%==5 GOTO WEBSERVICE
IF %option%==6 GOTO HOSERVICE
IF %option%==7 GOTO BRASERVICE
IF %option%==8 GOTO HOSERVER
IF %option%==9 GOTO EODSERVICE
IF %option%==10 GOTO UPDCOBRDET
IF %option%==99 GOTO MENU
IF %option%=="%option%" GOTO TOPGENSVR
goto TOPGENSVR

:ICSTSERVICE
@echo ***** CREATING IC STORE SYNC SERVICE*****
copy "\\%SERVER IP%\%NETSHARE%\GENESIS\GENSERVICES\genICStoreSvr.exe" "C:\GENESIS\GENSERVICES\genICStoreSvr%GENBR%.exe" /y
(
	ECHO echo off
	ECHO cls
	ECHO ======================
	ECHO set GENCOMP=%GENCOMP%
	ECHO set GENBR=%GENBR%
	ECHO ======================
	ECHO SETLOCAL EnableExtensions
	ECHO set EXE=genICStoreSvr%%GENBR%%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXE%%"'^) DO IF %%%%x == %%EXE%% goto FOUND
	ECHO echo SERVICE IS NOT RUNNING
	ECHO echo ***** STARTING BRANCH SYNC SERVICE *****
	ECHO echo %%GENBR%%
	ECHO copy "\\%SERVER IP%\%NETSHARE%\GENESIS\GENSERVICES\genICStoreSvr.exe" "C:\GENESIS\GENSERVICES\genICStoreSvr%%GENBR%%.exe" /y
	ECHO start "" "C:\GENESIS\GENSERVICES\%%EXE%%" %%GENCOMP%% %%GENBR%% AUTO
	ECHO goto EXIT
	ECHO :FOUND
	ECHO echo SERVICE IS RUNNING
	ECHO ping 127.0.0.1 -n 3 ^> nul
	ECHO exit
)>"C:\GENESIS\GENSERVICES\START-IC-STORE-SERVICE-%GENBR%.bat"
schtasks /create /sc DAILY /st 06:00:00 /RI 1 /DU 24:00 /tn "genesisICstoreSVR-%GENBR%" /RU "%SVRNAME%" /RP "%SVRPASS%" /tr "C:\GENESIS\GENSERVICES\START-IC-STORE-SERVICE-%GENBR%.bat"
cls
@echo ***** DONE *****
@echo ***** CREATING CHECK SERVICES STATUS *****
(
	ECHO @echo off
	ECHO SETLOCAL EnableExtensions
	ECHO set EXE1=gen*
	ECHO :top
	ECHO cls
	ECHO TASKLIST /FI "IMAGENAME eq %%exe1%%"
	ECHO @ECHO .
	ECHO @ECHO .
	ECHO @ECHO .
	ECHO timeout 60
	ECHO goto top
)>"C:\GENESIS\GENSERVICES\CHECK-SERVICES-STATUS.bat"

echo Set oWS = WScript.CreateObject("WScript.Shell") > CreateShortcut.vbs
echo sLinkFile = "%HOMEDRIVE%%HOMEPATH%\Desktop\CHECK SERVICES STATUS.lnk" >> CreateShortcut.vbs
