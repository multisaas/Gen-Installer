:CreateNEXUSDB
cls
@echo ***** CREATE GENESIS NEXUS DATABASE? *****(Y/N)
set INPUT=
set /P INPUT=TYPE INPUT: %=%
If %INPUT%== y goto yes1
If %INPUT%== Y goto yes1
If %INPUT%==%INPUT% goto MENU
:yes1
del "c:\scripts\nexus.txt"
@echo ***** CREATEING GENESIS NEXUS DATABASE AND NEXUS USER ***** >>"c:\scripts\nexus.txt"
@ECHO OFF
ECHO. > MultiLine.sql

::--THIS WILL CREATE YOUR DATABASE IF ITS NOT CREATED ALREADY.
::--STEP 1

ECHO --THIS IS CREATING NEXUS SQL DATABASE

ECHO USE master >> MultiLine.sql
ECHO GO >> MultiLine.sql
ECHO IF NOT EXISTS(SELECT * FROM sys.databases WHERE name = 'NEXUS') >> MultiLine.sql
ECHO BEGIN >> MultiLine.sql
ECHO CREATE DATABASE NEXUS >> MultiLine.sql
ECHO END >> MultiLine.sql

ECHO.
sqlcmd -S (LOCAL) -E -i MultiLine.sql >> "c:\scripts\nexus.txt"

::--THIS WILL CREATE YOUR USER FOR NEXUS READ ONLY.
::--STEP 2

ECHO --THIS IS CREATING NEXUS SQL USER

ECHO DECLARE @DatabaseName NVARCHAR(100) >> MultiLine.sql
ECHO DECLARE @SQL NVARCHAR(max) >> MultiLine.sql
ECHO DECLARE @User VARCHAR(64) >> MultiLine.sql
ECHO SET @User = 'NEXUS' >> MultiLine.sql
ECHO create login NEXUS with password='D0gm@t1x' ,CHECK_EXPIRATION = OFF,  CHECK_POLICY = OFF; >> MultiLine.sql
ECHO DECLARE Grant_Permission CURSOR LOCAL FOR >> MultiLine.sql
ECHO SELECT name FROM sys.databases >> MultiLine.sql
ECHO WHERE name NOT IN ('master','model','msdb','tempdb','distribution') >> MultiLine.sql
ECHO OPEN Grant_Permission >> MultiLine.sql
ECHO FETCH NEXT FROM Grant_Permission INTO @DatabaseName >> MultiLine.sql
ECHO WHILE @@FETCH_STATUS = 0 >> MultiLine.sql
ECHO BEGIN >> MultiLine.sql
ECHO SELECT @SQL = 'USE '+ '[' + @DatabaseName + ']' +'; '+ 'CREATE USER ' + @User + >> MultiLine.sql
ECHO ' FOR LOGIN ' + @User + '; EXEC sp_addrolemember N''db_datareader'', >> MultiLine.sql
ECHO ' + @User + ''; >> MultiLine.sql
ECHO PRINT @SQL >> MultiLine.sql
ECHO EXEC sp_executesql @SQL >> MultiLine.sql
ECHO FETCH NEXT FROM Grant_Permission INTO @DatabaseName >> MultiLine.sql
ECHO END >> MultiLine.sql
ECHO CLOSE Grant_Permission >> MultiLine.sql
ECHO DEALLOCATE Grant_Permission >> MultiLine.sql

ECHO.
sqlcmd -S (LOCAL) -E -i MultiLine.sql >> "c:\scripts\nexus.txt"

::--THIS WILL CREATE YOUR TABLE IN THE DATABASE.
::--STEP 3

:NEXUSSERVICE
cls
ECHO =========================================
ECHO PLEASE PROVIDE SERVER DETAIL THATS NEEDED
ECHO =========================================
set /p StoreDB=STORE DATABASE:
set /p CO=GENESIS COMPANY CODE:
set /p BRANCH=GENESIS BRANCH CODE:
set /p StoreName=STORE NAME:

ECHO USE [NEXUS] >> MultiLine.sql
ECHO GO >> MultiLine.sql
ECHO DECLARE @wStoreDB        VARCHAR(30) >> MultiLine.sql
ECHO DECLARE @wCO             VARCHAR(30) >> MultiLine.sql
ECHO DECLARE @wBranch         VARCHAR(10) >> MultiLine.sql
ECHO DECLARE @StorProc        VARCHAR(50)  >> MultiLine.sql
ECHO DECLARE @EMailTo         NVARCHAR(350)  >> MultiLine.sql
ECHO DECLARE @EMailCC         NVARCHAR(350)  >> MultiLine.sql
ECHO DECLARE @EMailBCC        NVARCHAR(350)  >> MultiLine.sql
ECHO DECLARE @StoreName       NVARCHAR(30)  >> MultiLine.sql
ECHO DECLARE @wCount          NVARCHAR(10) >> MultiLine.sql
ECHO DECLARE @GPCheck         NVARCHAR(3)  >> MultiLine.sql
ECHO DECLARE @DataSource      NVARCHAR(30)  >> MultiLine.sql
ECHO DECLARE @rmtUser         NVARCHAR (30)  >> MultiLine.sql
ECHO DECLARE @rmtPassword     NVARCHAR (30)  >> MultiLine.sql
ECHO SET @wStoreDB            = '%StoreDB%'                                                                                              --STORE DATABASE >> MultiLine.sql
ECHO SET @wCO                 = '%CO%'                                                                                                   --COMPANY CODE >> MultiLine.sql
ECHO SET @wBranch             = '%BRANCH%'                                                                                               --BRANCH CODE >> MultiLine.sql
ECHO SET @EMailTo             = ''                                                                                                       --STORE MANAGERS ONLY >> MultiLine.sql
ECHO SET @EMailCC             = ''                                                                                                       --OPTINAL STORE MANAGERS >> MultiLine.sql
ECHO SET @EMailBCC            = 'pg.mouton@mrjc.co.za;riaan.jacobs.co.za'                                                                --BCC MRJ MEMBERS ONLY >> MultiLine.sql
ECHO SET @StoreName           = '%StoreName%'                                                                                            --STORE NAME (FRIENDLY NAME) >> MultiLine.sql
ECHO SET @wCount              = '1'                                                                                                      --START COUNT DO NOT CHANGE IF ONLY ONE STORE >> MultiLine.sql
ECHO SET @GPCheck             = ''                                                                                                       --GP BELOW PERCENTAGE >> MultiLine.sql
ECHO SET @DataSource          = ''                                                                                                       --EXTERNAL SERVER >> MultiLine.sql
ECHO SET @rmtUser             = ''                                                                                                       --EXTERNAL SERVER SQL USER >> MultiLine.sql
ECHO SET @rmtPassword         = ''                                                                                                       --EXTERNAL SERVER SQL PASSWORD >> MultiLine.sql
ECHO CREATE TABLE [dbo].[Nexus]( >> MultiLine.sql
ECHO [StoreDB] [nvarchar](30) NULL, >> MultiLine.sql
ECHO [Company] [nvarchar](30) NULL, >> MultiLine.sql
ECHO [Branch] [nvarchar](10) NULL, >> MultiLine.sql
ECHO [StorProc] [varchar](50) NULL, >> MultiLine.sql
ECHO [EMailTo] [nvarchar](350) NULL, >> MultiLine.sql
ECHO [EMailCC] [nvarchar](350) NULL, >> MultiLine.sql
ECHO [EMailBCC] [nvarchar](350) NULL, >> MultiLine.sql
ECHO [StoreName] [nvarchar](30) NULL, >> MultiLine.sql
ECHO [wCount] [int] NULL, >> MultiLine.sql
ECHO [GPCheck] [nvarchar](3) NULL, >> MultiLine.sql
ECHO [DataSource] [nvarchar](30) NULL, >> MultiLine.sql
ECHO [rmtUser] [nvarchar](30) NULL, >> MultiLine.sql
ECHO [rmtPassword] [nvarchar](30) NULL >> MultiLine.sql
ECHO ) ON [PRIMARY] >> MultiLine.sql
ECHO INSERT INTO [dbo].[Nexus]  >> MultiLine.sql
ECHO ([StoreDB], [Company],[Branch],[StorProc],[EMailTo],[EMailCC],[EMailBCC],[StoreName],[wCount],[GPCheck],[DataSource],[rmtUser],[rmtPassword])  >> MultiLine.sql
ECHO VALUES >> MultiLine.sql
ECHO (@wStoreDB,@wCO,@wBranch,'NexusSalesStats',@EMailTo,@EMailCC,@EMailBCC,@StoreName,@wCount,@GPCheck,@DataSource,@rmtUser,@rmtPassword), >> MultiLine.sql
ECHO (@wStoreDB,@wCO,@wBranch,'NexusCheckOnhandBalance',@EMailTo,@EMailCC,@EMailBCC,@StoreName,@wCount,@GPCheck,@DataSource,@rmtUser,@rmtPassword); >> MultiLine.sql

ECHO.
sqlcmd -S (LOCAL) -E -i MultiLine.sql >> "c:\scripts\nexus.txt"

ECHO --THIS IS POPULATION THE DATABASE

del MultiLine.sql

goto MENU
