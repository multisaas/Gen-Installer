	ECHO set EXE=genICStoreSvr%%GENGRP%%.exe
	ECHO FOR /F %%^%%x IN ^('tasklist /NH /FI "IMAGENAME eq %%EXE%%"'^) DO IF %%%%x == %%EXE%% goto FOUND
	ECHO echo SERVICE IS NOT RUNNING
	ECHO echo ***** STARTING BRANCH SYNC SERVICE *****
	ECHO echo %%GENGRP%%
	ECHO copy "\\%SERVER IP%\%NETSHARE%\GENESIS\GENSERVICES\genICStoreSvr.exe" "C:\GENESIS\GENSERVICES\genICStoreSvr%%GENGRP%%.exe" /y
	ECHO start "" "C:\GENESIS\GENSERVICES\%%EXE%%" CAT %%GENGRP%% AUTO
	ECHO goto EXIT
	ECHO :FOUND
	ECHO echo SERVICE IS RUNNING
	ECHO ping 127.0.0.1 -n 3 ^> nul
	ECHO exit
)>"C:\GENESIS\GENSERVICES\START-IC-STORE-SERVICE-%GENGRP%.bat"
